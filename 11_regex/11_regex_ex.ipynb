    "hand = open('regex_sum_237566.txt')\n",
    "l=[]\n",
    "for line in hand:\n",
    "    x = re.findall('([0-9]+)', line)\n",
    "    if len(x) > 0:\n",
    "        for i in x:\n",
    "            l.append(int(i))\n",
    "print(sum(l))"
